(function(){var w=window;var o=w.ysm_customData;if(typeof(o)!="object"||o===null){o=w.pm_customData}var adCenterIds=['254955'];var segmentIds=['15EIADDS6DBQEC','1LPAN479OSD03M','11EO9LTB5O96RU','13R20K124K4S96','1D4LJ7POQ4VBM4','1JFG53UO9I8AJK','17JJDIJJ7T9CCC','1DA8D75G7M8B90','101LDISOC6NF5E','1GLC0FENS9NSEU','1E48OKR9EO3SDK','1TNJEBU320JGI4','10HJCDQCE80A88','1GSTKTBTH3M0BC','11UB4KJARQ3SS6','13G5SCI588K1LS','1NOK8USJFVMEM2','16KR57IRJ55JH2','1R3D8FLTHVM0U6','1VLBRHMTD3R18C'];var segId=-1;var evtId=-1;var bf=0;var amount="";if(typeof(w.ysm_bf)!="undefined"){bf=w.ysm_bf}if(typeof(o)=="object"&&o!==null){for(var d in o){if(typeof(d)=="string"&&d!==null&&typeof(o[d])=="string"&&o[d]!==null&&o[d].length>0){if(d=="conversion"){segId=0;evtId=1}else{if(d=="segment"){segId=0}else{if(d.indexOf("segment_")!=-1){segId=0;for(var i=0;i<segmentIds.length;i++){if(d.indexOf("segment_"+segmentIds[i])!=-1){segId=i;break}}}}}var a=o[d].split(",");for(var j=0;j<a.length;j++){if(a[j].indexOf("name=conversion")!=-1||a[j].indexOf("event=1")!=-1){evtId=1}else{if(a[j].indexOf("name=shopper")!=-1||a[j].indexOf("event=0")!=-1){evtId=0}}var nv=a[j].split("=");if(nv.length>1){var n=nv[0].toLowerCase();var v=nv[1];if(n.indexOf("amount")!=-1){amount=v}}}if(segId!=-1){break}}}}if(!window.mstag_imgs){window.mstag_imgs=[]}var ystage=(evtId!=-1?""+(evtId+3):(bf==1?"2":"0"));var ygoal=(segId!=-1?""+segId:"0");for(var aId in adCenterIds){if(adCenterIds.hasOwnProperty(aId)){var img=new Image();img.src="//"+adCenterIds[aId]+".r.msn.com/?type=1&dedup=1&piggyback=Y&domainID="+adCenterIds[aId]+"&YGoal="+ygoal+"&YStage="+ystage+"&Revenue="+amount;window.mstag_imgs.push(img)}}})();